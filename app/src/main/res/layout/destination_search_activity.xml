<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent"
    android:background="@color/white">

    <FrameLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_marginTop="@dimen/common_titlebar_height">
        <TextView
            android:id="@+id/destination_search_no_result"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:text="@string/search_no_result"
            android:textColor="@color/posting_main_text_gray"
            android:layout_marginTop="15dp"
            android:paddingLeft="15dp"
            android:textSize="@dimen/posting_main_text_size_1"
            android:gravity="center_horizontal"
            android:visibility="gone"/>

        <ListView
            android:id="@+id/destination_search_lv"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:visibility="gone"/>


        <!--<include-->
            <!--android:id="@+id/destination_search_post_list_result"-->
            <!--layout="@layout/activity_page_list"/>-->

    </FrameLayout>

    <com.elong.tourpal.ui.views.CommonTitleBar
        android:id="@+id/destination_search_titlebar"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"/>
</RelativeLayout>
